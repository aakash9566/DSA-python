class Node:
    def __init__(self,data):
        self.data=data
        self.next=None
class Queue:
    def __init__(self):
        self.front=None
        self.rear=None
    def is_empty(self):
        return self.front is None
    def en(self,data):
        new=Node(data)
        if self.rear is None:
            self.front=self.rear=new
            return
        self.rear.next=new
        self.rear=new
    def de(self):
        if self.is_empty():
            return None
        data=self.front.data
        self.front=self.front.next
        if self.front is None:
            self.rear=None
        return data
    def peek(self):
        if self.is_empty():
            return None
        return self.front.data
    def size(self):
        co=0
        cu=self.front
        while cu:
            co+=1
            cu=cu.next
        return co
q=Queue()
q.en(10)
q.en(20)
q.en(30)
print("Queue elements:",end=" ")
while not q.is_empty():
    print(q.de(),end=" ")
print("\n Is the queue empty?",q.is_empty())    
    
